<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.lv10z1.Controllers.GlavnaController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />

    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <TableView fx:id="tableViewGradovi" GridPane.columnSpan="4">
        <columns>
            <TableColumn fx:id="colGradId" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="colGradNaziv" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="Naziv" />
            <TableColumn fx:id="colGradStanovnika" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="Stanovnika" />
            <TableColumn fx:id="colGradDrzava" maxWidth="1.7976931348623157E308" prefWidth="75.0" text="Država" />
        </columns>
    </TableView>
    <VBox GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.rowSpan="4">
        <children>
            <Button fx:id="btnDodajGrad" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#dodajGrad" text="Dodaj grad" VBox.vgrow="ALWAYS" />
            <Button fx:id="btnDodajDrzavu" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#dodajDrzavu" text="Dodaj državu" VBox.vgrow="ALWAYS" />
            <Button fx:id="btnIzmijeniGrad" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#izmijeniGrad" text="Izmijeni grad" />
            <Button fx:id="btnObrisiGrad" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#obrisiGrad" text="Obriši grad" />
        </children>
        <GridPane.margin>
            <Insets />
        </GridPane.margin>
    </VBox>
</GridPane>
