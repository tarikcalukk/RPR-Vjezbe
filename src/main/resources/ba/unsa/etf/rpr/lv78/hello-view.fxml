<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.lv78.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <BorderPane GridPane.rowSpan="5">
        <center>
          <ListView fx:id="listKorisnici" BorderPane.alignment="CENTER" />
        </center>
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </BorderPane>
    <Label text="Ime:" GridPane.columnIndex="1">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </Label>
    <Label text="Prezime:" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </Label>
    <Label text="E-mail:" GridPane.columnIndex="1" GridPane.rowIndex="2">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </Label>
    <Label text="Korisničko ime: " GridPane.columnIndex="1" GridPane.rowIndex="3">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </Label>
    <Label text="Lozinka: " GridPane.columnIndex="1" GridPane.rowIndex="4">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </Label>
    <TextField fx:id="fldIme" GridPane.columnIndex="2">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </TextField>
    <TextField fx:id="fldPrezime" GridPane.columnIndex="2" GridPane.rowIndex="1">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </TextField>
    <TextField fx:id="fldMail" GridPane.columnIndex="2" GridPane.rowIndex="2">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </TextField>
    <TextField fx:id="fldKorIme" GridPane.columnIndex="2" GridPane.rowIndex="3">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </TextField>
    <PasswordField fx:id="fldLozinka" GridPane.columnIndex="2" GridPane.rowIndex="4">
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </PasswordField>
    <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5">
      <buttons>
          <Button fx:id="Dodaj" defaultButton="true" mnemonicParsing="false" onAction="#dodavanje" text="Dodaj" />
          <Button fx:id="Kraj" cancelButton="true" mnemonicParsing="false" onAction="#zavrsi" text="Kraj" />
      </buttons>
        <GridPane.margin>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </GridPane.margin>
    </ButtonBar>
</GridPane>
